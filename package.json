{
  "name": "@chambr/engine-core",
  "version": "0.1.0",
  "description": "Core runtime engine for Chambr rooms",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --clean",
    "lint": "tsc --noEmit",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.71",
    "ai": "^5.0.99",
    "zod": "^4.1.12"
  },
  "peerDependencies": {
    "@ai-sdk/openai": "^2.0.71",
    "ai": "^5.0.99"
  },
  "devDependencies": {
    "tsup": "^8.5.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18"
  },
  "license": "MIT"
}
